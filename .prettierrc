{
  "tabWidth": 2,
  "printWidth": 120,
  "endOfLine": "auto",
  "arrowParens": "always",
  "trailingComma": "none",
  "semi": true,
  "useTabs": false,
  "singleQuote": false,
  "bracketSpacing": true,
  "bracketSameLine": false,
  "singleAttributePerLine": true,
  "plugins": ["prettier-plugin-organize-imports"]
}
